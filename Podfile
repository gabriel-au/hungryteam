  use_frameworks!

  # Pods for HungryTeam
  pod 'Firebase'
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'

  target 'HungryTeamTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'HungryTeamUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
